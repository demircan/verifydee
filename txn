CREATE MEMORY TABLE PUBLIC.STUDENT(
    USER_ID VARCHAR(255) NOT NULL,
    EXPIRE_DATE DATE NOT NULL,
    LASTNAME VARCHAR(255),
    NAME VARCHAR(255),
    TCKNO VARCHAR(255),
    VERIFIED BOOLEAN
);
ALTER TABLE PUBLIC.STUDENT ADD CONSTRAINT PUBLIC.CONSTRAINT_B PRIMARY KEY(USER_ID);           
-- 0 +/- SELECT COUNT(*) FROM PUBLIC.STUDENT; 
CREATE MEMORY TABLE PUBLIC.TXN(
    OPERATION_ID VARCHAR(255) NOT NULL,
    BARKOD_NO VARCHAR(255),
    OPERATION_DATE DATE NOT NULL,
    TCKNO VARCHAR(255),
    VERIFIED BOOLEAN
);   
ALTER TABLE PUBLIC.TXN ADD CONSTRAINT PUBLIC.CONSTRAINT_1 PRIMARY KEY(OPERATION_ID);          
